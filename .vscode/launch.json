{
   // Use IntelliSense to learn about possible attributes.
   // Hover to view descriptions of existing attributes.
   // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
   "version": "0.2.0",
   "configurations": [
      {
         "cwd": "${workspaceRoot}",
         "executable": "Build/stm32_discovery.elf",
         "name": "STM32",
         "request": "launch",
         "type": "cortex-debug",
         "servertype": "openocd",
         "runToMain": true,
         "device": "STM32F407VGT6",
         "configFiles": [
            "stm32f407.cfg"
         ]
      },
      {
         "name": "GDB",
         "type": "cppdbg",
         "request": "launch",
         "program": "${workspaceRoot}/Build/stm32_discovery.elf",
         "args": [],
         "stopAtEntry": true,
         "cwd": "${workspaceFolder}",
         "environment": [],
         "externalConsole": false,
         "MIMode": "gdb",
         "miDebuggerPath": "C:/Program Files (x86)/GNU Arm Embedded Toolchain/9 2020-q2-update/bin/arm-none-eabi-gdb.exe",
         "autorun": [
            "target remote localhost:3333",
            "symbol-file Board/stm32_discovery.elf",
            "monitor reset"
         ],
         "setupCommands": [
            {
               "description": "Enable pretty-printing for gdb",
               "text": "-enable-pretty-printing",
               "ignoreFailures": true
            }
         ]
      }
   ]
}